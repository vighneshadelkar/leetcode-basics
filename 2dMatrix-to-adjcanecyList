
    const trust = [[1, 3], [2, 3]];
    
    let adjacencyList = {};

    const numNodes = Math.max(...trust.flat());
    const set = new Set([...trust.flat()]);


    for (let i = 0; i < numNodes; i++) {

        for (let [from, to] of trust) {
            adjacencyList[from] = [];
            adjacencyList[from].push(to);
        }
    }

OUTPUT:
    {
  1: [3],
  2: [3],
  3: []
}
